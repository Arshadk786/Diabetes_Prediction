{
    "version": 2,
    "builds": [
      {
        "src": "api/app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/predict",
        "dest": "api.app.predict_diabetes"
      },
      {
        "src": "/(.*)",
        "dest": "api.app.handler"
      }
    ]
  }
  